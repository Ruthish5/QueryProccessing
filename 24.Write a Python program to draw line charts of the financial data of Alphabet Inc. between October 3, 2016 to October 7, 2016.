import pandas as pd
import matplotlib.pyplot as plt
data = pd.read_csv('C:/Users/pedda/Downloads/QPDS/DAY3/Programs/fdata.csv')
data['Date'] = pd.to_datetime(data['Date'])
plt.plot(data['Date'], data['Open'], label='Open', color='blue')
plt.plot(data['Date'], data['High'], label='High', color='green')
plt.plot(data['Date'], data['Low'], label='Low', color='red')
plt.plot(data['Date'], data['Close'], label='Close', color='orange')
plt.xlabel('Date')
plt.ylabel('Price')
plt.title('Alphabet Inc. Financial Data (Oct 3-7, 2016)')
plt.legend()
plt.xticks(rotation=45)
plt.tight_layout()
plt.show()
