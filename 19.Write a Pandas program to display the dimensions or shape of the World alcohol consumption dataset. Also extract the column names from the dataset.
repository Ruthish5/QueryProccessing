import pandas as pd
data = {
    "year": [1986, 1986, 1986, 1987, 1987],
    "who region": ["Americas", "Europe", "Europe", "Asia", "Americas"],
    "country": ["USA", "France", "Germany", "India", "Mexico"],
    "beverage types": ["Beer", "Wine", "Beer", "Spirits", "Beer"],
    "displayvalue": [5.0, 7.1, 6.5, 0.2, 4.3]
}
df = pd.DataFrame(data)
dimensions = df.shape
column_names = df.columns.tolist()
print(f"Dimensions (rows, columns): {dimensions}")
print("Column names:", column_names)
